{% extends 'base.html.twig' %}
{% block body %}
    {% for category in categories %}
        <div class="card" style="width: 18rem;">
            <img src="{{ category.logo }}" class="card-img-top" alt="{{ category.name }}">
            <div class="card-body">
                <p class="card-text">
                    {{ category.name }}
                </p>
            </div>
        </div>
    {% endfor %}
{% endblock %}



